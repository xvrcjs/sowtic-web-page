{
  "name": "sowtic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://sowtic.com/",
  "scripts": {
    "dev": "vite --host --port 8080",
    "build": "tsc && vite build --base=./",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "bootstrap:migrate": "rimraf src/styles/vendor/bootstrap && cp -R node_modules/bootstrap/scss src/styles/vendor/bootstrap && npx sass-migrator module src/styles/vendor/bootstrap/**/*.scss && npx sass-migrator color-functions src/styles/vendor/bootstrap/**/*.scss"
  },
  "dependencies": {
    "@emailjs/browser": "^3.12.1",
    "bootstrap": "^5.3.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.2.7",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^6.20.1",
    "sort-by": "^1.2.0",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-google-recaptcha": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.69.5",
    "sass-migrator": "^1.69.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
